<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="proj.spring.mes.dao.mapper.ClientItemsMapperDAO">
  <select id="selectClientitemsByItem" parameterType="string"
          resultType="proj.spring.mes.dto.ClientitemsDTO">
  <![CDATA[
  SELECT
    client_id,               
    item_id
  FROM V3_CLIENT_ITEMS
  WHERE item_id = #{itemId}
  ORDER BY client_id
  ]]>
  </select>
</mapper>














